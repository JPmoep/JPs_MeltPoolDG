subsection spatial domain
  set dimension = 2
  set global refinements = 5
end
subsection level set equation
  set level set degree = 2
  set artificial diffusivity = 0
  set activate reinitialization = 1
  set max reinitialization steps = 5
end
subsection time stepping
  set theta            = 0.5
  set start time       = 0.0
  set end time         = 2.0
  set time step size   = 0.01
end
subsection output
  set output norm levelset = 1
  set output walltime  = 0
  set compute paraview output  = 0
  set compute volume output    = 0
  set filename volume output   = myVolumes.txt
end
